{
    "name": "cfb",
    "version": "0.2.0",
    "description": [
        "Canadian Furious Beaver - IRP interceptor and replayer"
    ],
    "dependencies": [
        {
            "name": "nlohmann-json"
        },
        {
            "name": "argparse"
        },
        {
            "name": "phnt"
        },
        {
            "name": "wil"
        }
    ],
    "default-features": [
        "gui",
        "tests"
    ],
    "features": {
        "gui": {
            "description": "The CFB GUI",
            "dependencies": [
                {
                    "name": "nlohmann-json"
                },
                {
                    "name": "imgui",
                    "features": [
                        "docking-experimental",
                        "dx12-binding",
                        "win32-binding"
                    ],
                    "platform": "windows"
                },
                {
                    "name": "phnt"
                },
                {
                    "name": "wil"
                }
            ]
        },
        "tools": {
            "description": "Some debug tools for CFB",
            "dependencies": [
                {
                    "name": "argparse"
                },
                {
                    "name": "wil"
                }
            ]
        },
        "tests": {
            "description": "Tests for CFB",
            "dependencies": [
                {
                    "name": "trompeloeil"
                },
                {
                    "name": "catch2"
                }
            ]
        }
    }
}
